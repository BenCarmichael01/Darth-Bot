{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"plugins": ["import", "prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"prettier/prettier": "off"
	},
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [".js", ".jsx", ".cjs", ".mjs"],
				"moduleDirectory": ["node_modules", "./"]
			}
		}
	}
}
